{
  "name": "slaszu/SimpleUser-Silex2",
  "type": "library",
  "description": "A simple database-backed user provider for Silex 2.0, with associated services and controllers forked from jasongrimes/SimpleUser.",
  "keywords": ["silex", "user", "user provider", "simple user provider"],
  "homepage": "https://github.com/DaveC49/SimpleUser-Silex2",
  "license": "BSD-2-Clause",
  "authors": [
    {
      "name": "Jason Grimes",
      "email": "jason@grimesit.com",
      "role": "Original Developer"
    },
    {
      "name": "David Cousens",
      "email":"davidcousens49@gmail.com",
      "role": "Developer - Mods to work with Silex 2.0"
    }
  ],
  "require": {
    "php": ">=5.6.0",
    "silex/silex": "~2.0",
    "symfony/security": "~2.8|^3.0",
    "doctrine/dbal": "~2.4",
    "symfony/twig-bridge": "~2.8|^3.0",
    "jasongrimes/paginator": "~1.0",
    "swiftmailer/swiftmailer": "~5.3"
  },
  "require-dev": {
    "phpunit/phpunit": "~5.4"
  },
  "autoload": {
    "psr-4": {"SimpleUser\\": "src/SimpleUser"}
  },
  "autoload-dev": {
    "psr-4": {"SimpleUser\\Tests\\": "tests/SimpleUser/Tests"}
  }
}
